{
  "name": "react-boilerplate",
  "version": "2.5.0",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-server",
    "build:dev": "cross-env NODE_ENV=development webpack --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --colors",
    "linter": "eslint src -f html --ext .jsx --ext .js -o lint-report.html",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-server": "cross-env NODE_ENV=production node server.js"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "dependencies": {
    "autoprefixer": "^7.0.0",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "del": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^11.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "redux": "^3.3.1",
    "redux-entity": "^5.0.5",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "sinon": "^2.2.0",
    "style-loader": "^0.18.1",
    "svg-react-loader": "^0.3.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "webpack-simple-progress-plugin": "0.0.2"
  }
}
